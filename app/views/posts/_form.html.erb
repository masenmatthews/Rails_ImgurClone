<%= render "layouts/errors", :object => @post %>

<%= form_for [@post], url: posts_path, html: { multipart: true } do |f| %>
  <%= f.file_field :image_file_name, as: :file %>
  <br>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br>
  <%= f.label :description %>
  <%= f.text_area :description %>
  <br>
  <hr>
  <%= f.submit %>
<% end %>
