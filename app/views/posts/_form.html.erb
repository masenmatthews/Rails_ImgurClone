<%= render "layouts/errors", :object => @post %>

<%= form_for [@post] do |f| %>
  <%= f.label :image %>
  <%= f.text_field :image %>
  <br>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br>
  <%= f.label :description %>
  <%= f.text_area :description %>
  <br>
  <hr>
  <%= f.submit %>
<% end %>
