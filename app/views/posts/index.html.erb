<h2>Posts</h2>

<% if @posts.any? %>
      <% @posts.each do |post| %>
      <h3><%= link_to post.title, post_path(post)  %></h3>
      <div class="col-md-6" id="images">
      <div class="row">
      <%= image_tag post.image.url%>
    </div>
  </div>
      <% end %>
  <% else %>
  <p>There are no posts yet!</p>
  <% end %>
